 
{{#if data}}
{{> userheaderafter}}
{{else}}
{{> userheader}}
{{/if}}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<link rel="stylesheet" href="/css/productlist.css" type="text/css">

<section class="product spad">
    <div class="container">
        <div class="row">
            



<section class="product spad">
    
    <div class="container">
        
        <div class="row">
             
          <div class="row property__gallery">
            {{#each mensdata}}
            <div class="col-lg-4 col-md-4 col-sm-6 mix women">
               
                <div class="product__item">
                    <div class="product__item__pic set-bg" data-setbg="images/{{this.image.[0]}}">
                        <div class="label new">New</div>
                        <ul class="product__hover">
                            <li><a href="/images/{{this.image.[0]}}" class="image-popup"><span class="arrow_expand"></span></a></li>
                            <li><a href="/addwishlist?id={{this._id}}"><span class="icon_heart_alt"></span></a></li>
                            <li><a href="#"><span class="icon_bag_alt"></span></a></li>
                        </ul>
                    </div>
                    <div class="product__item__text">
                        <h6><a href="/productdetails?id={{this._id}}">{{this.name}}</a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div >Rs. {{this.price}}</div>
                    </div>
                </div>
               
            </div>
            {{/each}}
        </div>
         
        </div>
         
    </div>
   
</section>








{{> userfooter}}